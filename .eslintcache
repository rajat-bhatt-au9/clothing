ssword(email, password);\r\n      this.setState({ email: \"\", password: \"\" });\r\n    } catch (error) {\r\n      console.log(\r\n        error\r\n      ); /* \r\n            this.setState({loginError: error.message}) */\r\n      switch (error.code) {\r\n        case \"auth/user-not-found\":\r\n          this.setState({ loginError: \"User Not Found\" });\r\n          break;\r\n      }\r\n    }\r\n  };\r\n\r\n  handleChange = (event) => {\r\n    const { value, name } = event.target;\r\n\r\n    this.setState({ [name]: value });\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"sign-in\">\r\n        <h2>I already have an account</h2>\r\n        <span>Sign in with your email and password</span>\r\n\r\n        <div className=\"error\">\r\n          <h2> {this.state.loginError} </h2>\r\n        </div>\r\n\r\n        <form onSubmit={this.handleSubmit}>\r\n          <FormInput\r\n            name=\"email\"\r\n            type=\"email\"\r\n            handleChange={this.handleChange}\r\n            value={this.state.email}\r\n            label=\"Email\"\r\n            required\r\n          />\r\n\r\n          <FormInput\r\n            name=\"password\"\r\n            type=\"password\"\r\n            handleChange={this.handleChange}\r\n            label=\"Password\"\r\n            value={this.state.password}\r\n            required\r\n          />\r\n\r\n          <div className=\"buttons\">\r\n            <CustomButton type=\"submit\">Sign In</CustomButton>\r\n            <CustomButton\r\n              onClick={signInWithGoogle}\r\n              type=\"button\"\r\n              isGoogleSignIn\r\n            >\r\n              Sign In With Google\r\n            </CustomButton>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default SignIn;\r\n","E:\\react projects\\clothing\\src\\components\\form-input\\FormInput.js",[],"E:\\react projects\\clothing\\src\\components\\custom-button\\CustomButton.js",[],"E:\\react